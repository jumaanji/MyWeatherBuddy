    <style name="SecondTheme">
        <item name="android:textColor">#515151</item>
        <item name="colorPrimary">#16820A</item>
        <item name="colorPrimaryDark">#16820A</item>
    </style>

    <style name="ThirdTheme">
        <item name="android:textColor">#515151</item>
        <item name="colorPrimary">#0000F0</item>
        <item name="colorPrimaryDark">#0000F0</item>
    </style>

</resources>
